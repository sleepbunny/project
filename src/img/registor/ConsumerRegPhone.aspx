        function countdownTimer(interval, $timer, beginEvent, endEvent) {
            if (beginEvent) {
                beginEvent();
            }
            var endtime = new Date();
            endtime.setMinutes(endtime.getMinutes() + 2);
            var timerId = null;
            var job = function () {
                var nowtime = new Date();
                var leftSecond = parseInt((endtime - nowtime.getTime()) / 1000);

                if (leftSecond > 0) {
                    $timer.html(leftSecond);
                }
                else {
                    $timer.html(0);
                }
                if (leftSecond <= 0) {
                    if (endEvent) {
                        endEvent();
                    }
                    clearInterval(timerId);
                }
            };
            job();
            timerId = setInterval(job, interval);
        }

