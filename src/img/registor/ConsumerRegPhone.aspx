        var SendToMobileCode = function () {
            var memberUrl = RSECGlobal.MemberSite;
            if ('https:' == document.location.protocol) {
                memberUrl = RSECGlobal.MemberSite.replace("http:", "https:");
            };
            if (jQueryValidateMobile.Check()) {
                var MobileNumber = $("#MainPlace_txt_Phone").val().trim();
                var verifyCode = $("#MainPlace_VerifyControl1_ctl00_txt_Code").val().trim();
                var verifyControlID = "/Customer/ConsumerRegPhone.aspx_VerifyControl1";
                jQuery.ajax({
                    url: memberUrl + "Customer/AsyncConsumerRegister.ashx?action=SendMobileRegisterCode",
                    data: { mobile: MobileNumber, verifyCode: verifyCode, verifyControlID: verifyControlID },
                    type: "GET",
                    timeout: 10000,
                    dataType: "jsonp",
                    jsonp: "callback",
                    success: function (callback, textStatus, object) {
                        if (callback.Data.success) {
                            $("#showSendCode").show();
                            countdownTimer(900, $("#cd_timer"), function () { $("#SendCode").unbind("click") }, function () { $("#SendCode").click(SendToMobileCode) });
                        }
                        else {
                            $("#VerifyControlError").html("<span class='dsfs' style='width:72px;margin-left: 120px;'>" + callback.Data.ErrorInfo + "</span>").show();
                            $("#showSendCode").hide();
                        }
                    },
                    error: function (xmlHttpRequest, textStatus, errorThrown) { }
                });
            }
        };

        function ValidateMobileCode() {
            var memberUrl = RSECGlobal.MemberSite;
            if ('https:' == document.location.protocol) {
                memberUrl = RSECGlobal.MemberSite.replace("http:", "https:");
            };
            var mobile = $("#MainPlace_txt_Phone").val().trim();
            var code = $("#MainPlace_txtMobileCode").val().trim();
            if (code != "") {
                jQuery.ajax({
                    url: memberUrl + "Customer/AsyncConsumerRegister.ashx?action=ValidateMobileRegisterCode",
                    data: { mobile: mobile, code: code },
                    type: "GET",
                    timeout: 10000,
                    dataType: "jsonp",
                    jsonp: "callback",
                    success: function (callback, textStatus, object) {
                        if (!callback.Data.success) {
                            $("#hidMobileCode").val("N");
                            $("#imgMobileCode").hide();
                            $("#span_CheckMobileCode").html("<span class='dsfs'>手机验证码不正确</span>").show();
                        }
                        else {
                            $("#hidMobileCode").val("Y");
                            $("#span_CheckMobileCode").hide();
                            $("#imgMobileCode").show();
                        }
                    }
                });
            }
        };
